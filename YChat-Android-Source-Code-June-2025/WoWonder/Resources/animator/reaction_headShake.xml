<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">

	<objectAnimator
        android:propertyName="translationX"
        android:valueType="floatType"
        android:valueFrom="0"
        android:valueTo="-2"
        android:duration="130"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
        android:propertyName="rotationY"
        android:valueType="floatType"
        android:valueFrom="0"
        android:valueTo="-9"
        android:duration="130"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
        android:propertyName="translationX"
        android:valueType="floatType"
        android:valueFrom="-2"
        android:valueTo="1"
        android:startOffset="130"
        android:duration="130"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
        android:propertyName="rotationY"
        android:valueType="floatType"
        android:valueFrom="-9"
        android:valueTo="7"
        android:startOffset="130"
        android:duration="390"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
        android:propertyName="translationX"
        android:valueType="floatType"
        android:valueFrom="1"
        android:valueTo="-1"
        android:startOffset="520"
        android:duration="130"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
        android:propertyName="rotationY"
        android:valueType="floatType"
        android:valueFrom="7"
        android:valueTo="-5"
        android:startOffset="520"
        android:duration="520"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
        android:propertyName="translationX"
        android:valueType="floatType"
        android:valueFrom="-1"
        android:valueTo="1"
        android:startOffset="1040"
        android:duration="130"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
        android:propertyName="rotationY"
        android:valueType="floatType"
        android:valueFrom="-5"
        android:valueTo="3"
        android:startOffset="1040"
        android:duration="780"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
        android:propertyName="translationX"
        android:valueType="floatType"
        android:valueFrom="1"
        android:valueTo="0"
        android:startOffset="1820"
        android:duration="130"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
        android:propertyName="rotationY"
        android:valueType="floatType"
        android:valueFrom="3"
        android:valueTo="0"
        android:startOffset="1820"
        android:duration="130"
        android:interpolator="@android:interpolator/fast_out_slow_in" />

</set>