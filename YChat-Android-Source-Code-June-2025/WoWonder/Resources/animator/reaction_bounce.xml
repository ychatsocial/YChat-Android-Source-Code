<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">

	<objectAnimator
		android:propertyName="translationY"
		android:valueType="floatType"
		android:valueFrom="0"
		android:valueTo="-2"
		android:duration="1300"
		android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
		android:propertyName="scaleY"
		android:valueType="floatType"
		android:valueFrom="1"
		android:valueTo="1.1"
		android:startOffset="260"
		android:duration="520"
		android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
		android:propertyName="translationY"
		android:valueType="floatType"
		android:valueFrom="-2"
		android:valueTo="-1"
		android:startOffset="780"
		android:duration="260"
		android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
		android:propertyName="scaleY"
		android:valueType="floatType"
		android:valueFrom="1.1"
		android:valueTo="1.05"
		android:startOffset="780"
		android:duration="260"
		android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
		android:propertyName="scaleY"
		android:valueType="floatType"
		android:valueFrom="1.05"
		android:valueTo="0.95"
		android:startOffset="1040"
		android:duration="260"
		android:interpolator="@android:interpolator/fast_out_slow_in" />

	<objectAnimator
		android:propertyName="scaleY"
		android:valueType="floatType"
		android:valueFrom="0.95"
		android:valueTo="1.02"
		android:startOffset="1300"
		android:duration="130"
		android:interpolator="@android:interpolator/fast_out_slow_in" />

</set>